{
    "autoload": {
        "psr-4": {
            "Jaspur\\LocalizedExceptions\\": "src/"
        }
    },
    "description": "A custom exception handler package for Laravel that provides localized error messages for HTTP exceptions.",
    "extra": {
        "laravel": {
            "providers": [
                "Jaspur\\LocalizedExceptions\\LocalizedExceptionsServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "name": "jaspur/localized-exceptions",
    "require": {
        "illuminate/auth": "^11.0",
        "illuminate/foundation": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "php": "^8.2",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0"
    },
    "type": "library"
}